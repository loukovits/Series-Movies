{% extends 'templates/app.twig'%}

{% block content %}
     {% for series in allSeries %}
           {{ series.name}}
            <br/>
           {{series.description}}
           <br/>
           {% for tag in series.tags %}
           {# {{ dump(tag) }}#}
           {{ tag.value }}
           <br/>
           {% endfor %}
           {% for image in series.images %}
               {{ image['filename'] }}
           <br/>
           {% endfor %}
           {% for actor in series.actors %}
              {{ actor.full_name }}
           {% endfor %}
           
           <br/>
           <hr>
     {% endfor %}
{% endblock %}